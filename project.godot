config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "Module",
"language": "GDScript",
"path": "res://World/LODTerrain/Modules/AbstractModule.gd"
} ]
_global_script_class_icons={
"Module": ""
}

[ServerConnection]

*res://Scripts/ServerConnection.gd=""

[application]

config/name="LandscapeLab!"
run/main_scene="res://StartScene.tscn"
boot_splash/image="res://Resources/Images/UI/Meta/splash.png"
boot_splash/fullsize=false
config/icon="res://Resources/Images/UI/Meta/icon.png"

[autoload]

Startup="*res://Global/Startup.gd"
Settings="*res://Global/Settings.gd"
logger="*res://Global/Debug/logger.gd"
ErrorPrompt="*res://Global/Debug/ErrorPrompt.gd"
GlobalSignal="*res://Global/GlobalSignal.gd"
ServerConnection="*res://Global/ServerConnection.gd"
ThreadPool="*res://Global/ThreadPool/ThreadPool.gd"
Session="*res://Global/Session.gd"
PlayerInfo="*res://Global/PlayerInfo.gd"
CachingImageTexture="*res://Global/CachingImageTexture.gd"
Offset="*res://Global/Offset.gd"
TooltipHandler="*res://Global/TooltipHandler.gd"
WorldPosition="*res://Global/WorldPosition.gd"
PerformanceTracker="*res://Global/Debug/PerformanceTracker.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "dscn_io" )

[gdnative]

singletons=[ "res://addons/godot-openvr/godot_openvr.gdnlib" ]

